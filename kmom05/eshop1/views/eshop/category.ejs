<%- include("../partials/header.ejs", {title: title}); %>
<div class="bg"></div>
<div class="bg bg2"></div>
<div class="bg bg3"></div>

<div class="content">
    <h1>Kategorier</h1>

    <p>Kategori Lista</p>
    
    <table>
        <tr>
            <th>Produkt</th>
            <th>Kategori</th>
        </tr>
        <% for (const row of res) { %>
        <tr>
            <td><%= row.produkt %></td>
            <td><%= row.kategori %></td>
        </tr>
        <% }; %>
    </table>
    
</div>

<%- include("../partials/footer.ejs"); %>