<%- include("../partials/header.ejs", {title: title}); %>
<div class="bg"></div>
<div class="bg bg2"></div>
<div class="bg bg3"></div>
<div class="content">
    <h1>Medlemmar</h1>

    <table>
        <tr>
            <th>Fornamn</th>
            <th>Efternamn</th>
            <th>Alias</th>
            <th>Ort</th>
            <th>Hund namn</th>
            <th>Ras</th>
            <th>Godkänd</th>
        </tr>
        <% for (const row of res) { %>
        <tr>
            <td><%= row.fornamn %></td>
            <td><%= row.efternamn %></td>
            <td><%= row.alias %></td>
            <td><%= row.ort %></td>
            <td>
                <a href="<%= row.url %>"><%= row.namn %></a>
            </td>
            <td><%= row.ras %></td>
            <td><%= row.godkänd %></td>
        </tr>
        <% }; %>
    </table>
</div>
<%- include("../partials/footer.ejs"); %>
