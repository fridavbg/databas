<%- include("../partials/header.ejs", {title: title}); %>
<div class="bg"></div>
<div class="bg bg2"></div>
<div class="bg bg3"></div>

<div class="content">
    <h1>Svensk Rock</h1>
    <p>Spel Schema</p>

    <table>
        <tr>
            <th>Artist ID</th>
            <th>Artist namn</th>
            <th>Ort</th>
            <th>Landskod</th>
            <th>Land</th>
            <th>Klockslag</th>
            <th>Datum</th>
            <th>Dag</th>
        </tr>
        <% for (const row of res) { %>
        <tr>
            <td><%= row.id %></td>
            <td><%= row.namn %></td>
            <td><%= row.ort %></td>
            <td><%= row.landskod %></td>
            <td><%= row.land %></td>
            <td><%= row.klockslag %></td>
            <td><%= row.datum %></td>       
            <td><%= row.dag %></td>
        </tr>
        <% }; %>
    </table>
</div>

<%- include("../partials/footer.ejs"); %>
