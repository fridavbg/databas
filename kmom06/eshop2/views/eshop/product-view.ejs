<%- include("../partials/header.ejs", {title: title}); %>
<div class="bg"></div>
<div class="bg bg2"></div>
<div class="bg bg3"></div>
<div class="content">
    <h1>Product overview</h1>
    <table>
        <tr>
            <th>Produktkod</th>
            <th>Produktnamn</th>
            <th>Produktbeskrivning</th>
            <th>Produktpris</th>
        </tr>
        <% for (const row of res) { %>
        <tr>
            <td><%= row.produktkod %></td>
            <td><%= row.produktnamn %></td>
            <td><%= row.produktbeskrivning %></td>
            <td><%= row.produktpris %> kr</td>
        </tr>
        <% }; %>
    </table>
    <a href="/eshop/crud" class="link">Go back</a>
</div>

<%- include("../partials/footer.ejs"); %>