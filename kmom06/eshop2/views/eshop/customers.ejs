<%- include("../partials/header.ejs", {title: title}); %>
<div class="bg"></div>
<div class="bg bg2"></div>
<div class="bg bg3"></div>
<div class="content">
    <h1>Customer overview</h1>
    <table>
        <tr>
            <th>Id</th>
            <th>Förnamn</th>
            <th>Efternamn</th>
            <th>Adress</th>
            <th>Telefonnummer</th>
            <th>Mail</th>
        </tr>
        <% for (const row of res) { %>
        <tr>
            <td><%= row.id %></td>
            <td><%= row.fornamn %></td>
            <td><%= row.efternamn %></td>
            <td><%= row.adress %></td>
            <td><%= row.telefonnummer %></td>
            <td><%= row.mail %></td>
            <td>
                Order
                <!-- Gör om till länk -->
            </td>
        </tr>
        <% }; %>
    </table>
    <a href="/eshop/crud" class="link">Go back</a>
</div>

<%- include("../partials/footer.ejs"); %>